{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf4lsd"
		},
		"LinkService_DataBase_Source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LinkService_DataBase_Source'"
		},
		"LinkService_Storage_Sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkService_Storage_Sink'"
		},
		"LinkService_DataBase_Source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "lsdeh-server.database.windows.net"
		},
		"LinkService_DataBase_Source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "OrdersDB"
		},
		"LinkService_DataBase_Source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminfarhan"
		},
		"LinkService_Storage_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa4adflsd.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf4lsd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkService_DataBase_Source_password": {
						"value": "[parameters('LinkService_DataBase_Source_password')]"
					},
					"LinkService_Storage_Sink_accountKey": {
						"value": "[parameters('LinkService_Storage_Sink_accountKey')]"
					},
					"LinkService_DataBase_Source_properties_typeProperties_server": {
						"value": "[parameters('LinkService_DataBase_Source_properties_typeProperties_server')]"
					},
					"LinkService_DataBase_Source_properties_typeProperties_database": {
						"value": "[parameters('LinkService_DataBase_Source_properties_typeProperties_database')]"
					},
					"LinkService_DataBase_Source_properties_typeProperties_userName": {
						"value": "[parameters('LinkService_DataBase_Source_properties_typeProperties_userName')]"
					},
					"LinkService_Storage_Sink_properties_typeProperties_url": {
						"value": "[parameters('LinkService_Storage_Sink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}